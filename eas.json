{
  "cli": {
    "version": ">= 5.8.0"
  },
  "build": {
    "development": {
      "developmentClient" : "true",
      "environment": "development",
      "ios": {
        "simulator": "true"
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_ENV": "preview"
      }
    },
    "production": {
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
